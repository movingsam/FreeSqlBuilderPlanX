@using FreeSqlBuilder.TemplateEngine.Utilities
@model FreeSqlBuilder.TemplateEngine.IBuilderTask
@{
    Layout = "../Share/_Layout.cshtml";
    var table = Model.CurrentTable;
    var dtoBuilder = Model.Project.Builders.FirstOrDefault(x => x.Name == "Dto");
    var dataType = $"IEnumerable<{dtoBuilder?.GetName(table.CsName)}>";
    var dtoNameSpace = $"using {Model?.Project?.ProjectInfo?.NameSpace}.{dtoBuilder?.ReplaceTablePath(table.CsName).Replace("/",".")};";
    var pkTypeStr = table.Primarys.FirstOrDefault()?.CsType.Name;
    var tKey = pkTypeStr == "Guid" ? "" : $",{pkTypeStr}";
    var pageViewDto = $"PageViewBase<{dtoBuilder?.GetName(table.CsName)}{tKey}>";
}
using System.Collections;
using System.Collections.Generic;
using FreeSql.DataAnnotations;
@dtoNameSpace
@Model.GetUsing()

namespace @(Model.GetNameSpace())
{
    @Model.CurrentTable.Comment.GetCSharpSummary()
    public class @Model.GetCodeFirstName() : @pageViewDto
    {
        public @Model.GetCodeFirstName() (@dataType datas,IPage page,long total):base(datas,page,total)
        {
        }
    }
}